{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint-react",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": false,
        "trailing-comma": false,
        "import-name": false,
        "ter-indent": [
            true,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "variable-name": false,
        "jsx-alignment": false,
        "jsx-equals-spacing": [
            true,
            "never"
        ],
        "jsx-self-close": true,
        "jsx-no-string-ref": false,
        "jsx-wrap-multiline": true,
        "radix": false,
        "object-literal-shorthand": false
    },
    "rulesDirectory": []
}